{{!< default}}
<main class="blog-main" role="main">
    <div class="wrapper">
        <article class="{{post_class}}">
            {{#post}}
            <header class="post-header">
                <time class="post-time" datetime="{{date format='YYYY-MM-DD'}}">{{date format='MMM DD, YYYY'}}</time>
                <h1 class="post-title"><a href="{{url}}">{{{title}}}</a></h1>
            </header>
            <section class="post-content">
                {{content}}
            </section>
            {{/post}}
        </article>
    </div>
</main>
